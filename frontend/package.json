  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --liveReload=true --proxy-config proxy.conf.json",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production",
    "test": "ng test",
